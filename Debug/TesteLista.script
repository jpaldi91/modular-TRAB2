=excluirelem       0         0

== Excluir elemento do início
=obtervalorelem    0   0    'a'    1
=excluirelem       0         0

== Excluir elemento final, único 
=obtervalorelem    0   1    'b'    1
=excluirelem       0         0

== Ver se a lista ficou vazia
=obtervalorelem    0   8    's'    3

== Destruir a lista
=destruirlista     0     0

== Criar uma lista com dois elementos
=criarlista        1    0
=inselemapos       1    0   0
=inselemapos       1    1   0

== Esvaziar a lista e ver se ficou vazia
=esvaziarlista     1    0
=obtervalorelem    1    5    'l'     3

== Destruir a lista
=destruirlista   1    0

== Criar uma lista com quatro elementos
=criarlista      2    0
=inselemapos     2    0   0
=inselemapos     2    1   0
=inselemapos     2    2   0
=inselemapos     2    3   0

== Verificar a lista de trás para diante
=obtervalorelem       2    3    'd'    1
=avancarelem          2   -1     0
=obtervalorelem       2    2    'c'    1
=avancarelem          2   -1     0
=obtervalorelem       2    1    'b'    1
